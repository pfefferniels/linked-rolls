  },
  "devDependencies": {
    "typescript": "^5.6.0-dev.20240809",
    "vite": "^5.0.8",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "ajv": "^8.17.1",
    "aton": "^0.0.4",
    "fast-xml-parser": "^4.4.1",
    "immer": "^10.1.3",
    "midifile-ts": "^1.5.1",
    "ts-json-schema-generator": "^2.4.0",
    "uuid": "^10.0.0"
  }
}
