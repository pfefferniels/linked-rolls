        },
        {
          "$ref": "#/definitions/HandwrittenText"
        },
        {
          "$ref": "#/definitions/Stamp"
        },
        {
          "$ref": "#/definitions/Cover"
        },
        {
          "$ref": "#/definitions/RollLabel"
        }
      ]
    },
    "Argumentation": {
      "description": "An argumentation provides reasons for a belief and  may be associated with an person carrying out that argumentation.",
      "properties": {
        "actor": {
          "$ref": "#/definitions/Person"
        },
        "note": {
          "type": "string"
        },
