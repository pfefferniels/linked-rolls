    },
    "Certainty": {
      "description": "Certainty levels for beliefs.",
      "enum": [
        "true",
        "likely",
        "possible",
        "unlikely",
        "false"
      ],
      "type": "string"
    },
    "CollationTolerance": {
      "description": "Tolerance used in collation of roll copies.",
