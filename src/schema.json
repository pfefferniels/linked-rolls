        "SoftPedalOn",
        "SoftPedalOff",
        "MezzoforteOff",
        "MezzoforteOn",
        "SlowCrescendoOn",
        "SlowCrescendoOff",
        "ForzandoOn",
        "ForzandoOff",
        "MotorOff",
        "MotorOn",
        "Rewind",
        "ElectricCutOff"
